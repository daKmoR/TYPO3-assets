<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<main>
			<ROOT>
				<TCEforms>
					<sheetTitle>Settings</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.DisplayAssetsDirectories.root>
						<TCEforms>
							<exclude>1</exclude>
							<label>Root Path</label>
							<config>
								<type>input</type>
								<eval>trim</eval>
								<size>48</size>
								<wizards type="array">
									<_PADDING>2</_PADDING>
									<link type="array">
										<type>popup</type>
										<title>Link</title>
										<icon>link_popup.gif</icon>
										<script>browse_links.php?mode=wizard&amp;act=folder</script>
										<params type="array">
											<blindLinkOptions>page,url,mail,spec</blindLinkOptions>
										</params>
										<JSopenParams>height=600,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
									</link>
								</wizards>
							</config>
						</TCEforms>
					</settings.DisplayAssetsDirectories.root>
					<settings.DisplayAssetsDirectories.templatePath>
						<TCEforms>
							<label>Template Path</label>
							<config>
								<type>input</type>
								<size>48</size>
							</config>
						</TCEforms>
					</settings.DisplayAssetsDirectories.templatePath>
				</el>
			</ROOT>
		</main>
	</sheets>
</T3DataStructure>